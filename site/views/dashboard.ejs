      </div>

      <a href="/loja">
        <div class="item">
          <i class="fas fa-dog"></i>
          PetShop
        </div>
      </a>
      <a href="/loja">
        <div class="item">
          <i class="fas fa-home"></i>
          Casa
        </div>
      </a>
      <a href="/loja">
        <div class="item">
          <i class="fas fa-bookmark"></i>
          Banner
        </div>
      </a>

      <div class="discord-end">
        <a href="/logout">
          <div class="item" style="color: rgb(242, 30, 58);">
            <i class="fas fa-sign-out-alt"></i>
            logout
          </div>
        </a>
      </div>
    </div>
  </div>




